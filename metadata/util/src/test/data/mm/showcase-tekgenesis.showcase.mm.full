form ChartDataShowcase "Charts Data" 
    on_load data
{
    header {
        message(title), col 6;
         "View Source" : label, link "/sg/view/source/tekgenesis.showcase.ChartDataShowcase", style "pull-right";
    };
    size    "Size"        : horizontal, style "marginRight10" {
        resolution3 "1280x720" : button, on_click size1280, style "marginRight10";
        resolution2 "854x480"  : button, on_click size854, style "marginRight10";
        resolution1 "640x480"  : button, on_click size640, style "marginRight10";
        resolution0 "320x240"  : button, on_click size320;
    };
    options "Options"     : horizontal {
        barWidth                               : Real, text_field(30), check barWidth <= 1 : "Must be less or equals to 1.", col 1,
                                                 default 0.8, hint "Bar Width", on_ui_change changeWidth;
        seriesMode     "Series Mode"           : SampleSeriesMode, combo_box, col 1, default STACKED, on_ui_change stack;
        hoverable      "Hoverable?"            : toggle_button, default true, on_ui_change hoverable, style "marginRight10";
        legend         "Legend?"               : toggle_button, default true, on_ui_change legend, style "marginRight10";
        showLabels     "Show labels on hover?" : toggle_button, default false, on_ui_change showLabels, style "marginRight10";
        verticalLabels "Vertical labels?"      : toggle_button, default false, on_ui_change toVerticalLabels;
        stepped        "Stepped?"              : toggle_button, default false, on_ui_change toStepped;
    };
    colors  "Colors"      : horizontal {
        defaultColors "Default"  : button, on_click defaultColors, style "marginRight10";
        red           "Red"      : button, on_click red, style "marginRight10";
        blue          "Blue"     : button, on_click blue, style "marginRight10";
        green         "Green"    : button, on_click green, style "marginRight10";
        palette1      "Palette1" : button, on_click palette1, style "marginRight10";
        palette2      "Palette2" : button, on_click palette2, style "marginRight10";
        palette3      "Palette3" : button, on_click palette3;
    };
    series  "Series"      : horizontal {
        showInt        "Int"         : check_box, col 4, default true;
        showReal       "Real"        : check_box, col 4, default true;
        showBigDecimal "Big Decimal" : check_box, col 4, default true;
    };
    column  "Chart"       : String, chart(line), on_click doClick {
        key                     : String, internal;
        label                   : text_field(255);
        integer    "Int"        : Int, text_field(12), hide_column when !(showInt), mask decimal;
        real       "Real"       : Real, text_field(30), hide_column when !(showReal);
        bigDecimal "BigDecimal" : Decimal(5, 2), text_field(7), hide_column when !(showBigDecimal), mask decimal;
    };
    current               : message;
    data    "Reload data" : button, on_click reloadData;
}

