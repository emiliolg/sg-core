widget BinaryNode "Binary Node" {
    vertical {
        vertical, col 0, style "center" {
            value     : String, internal, col 0;
            button    : toggle_button, label_expression value, on_ui_change toggle;
        };
        horizontal, col 0 {
            left     : String, widget(BinaryNode), col 6, optional;
            right    : String, widget(BinaryNode), col 6, optional;
        };
    };
    inorder    : String, display(255), content_style "margin-left-15", is (((left.inorder + " ") + value) + " ") + right.inorder,
                 style "center";
}

form BinaryTree "Binary Tree" 
{
    insert    "Choose to insert" : vertical, hide when rows(ref) == 0 {
        letters    : String, section, col 0 {
            ref        : String, internal;
            element    : button, inline_style "float:left;width:75px", label_expression ref, on_click select;
        };
    };
    tree      "Tree"             : vertical {
        head    : String, widget(BinaryNode), col 0, optional;
    };
    selection "Selection"        : vertical {
        input_group, col 0, hide when selected == null {
            selected            : text_field(255), col 1, disable, optional;
            deselect "Deselect" : button, on_click deselect;
        };
    };
    order     "Tree inorder"     : vertical {
        inorder    : String, display(255), col 0, is head.inorder;
    };
}

