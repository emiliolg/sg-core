    optionalCheckField "Field"                       : text_field(20), optional when optionalCheck;
    fromField          "Start Date"                  : date_box, required;
    toField            "End Date"                    : date_box,
                                                       check toField > fromField : "Start date should be earlier than End date",
                                                       required;
                       "Validation Types"            : vertical {
        input1 "Default error"   : Int, text_field(12), check (input1 > 10 : "Must be over 10", input1 < 15 : "Must be under 15"),
                                   col 0, hint "check someNumber > 10 <message>, check someNumber < 15 <message>", mask decimal;
        input2 "Default warning" : Int, text_field(12), check input2 > 10 : warning "Must be over 10", col 0,
                                   hint "check someNumber > 10 : warning <message>", mask decimal;
        input3 "Default info"    : Int, text_field(12), check input3 > 10 : info "Must be over 10", col 0,
                                   hint "check someNumber > 10 : info <message>", mask decimal;
        input4 "Inline error"    : Int, text_field(12), check input4 > 10 : inline "Must be over 10", col 0,
                                   hint "check someNumber > 10 : inline <message>", mask decimal;
        input5 "Inline warning"  : Int, text_field(12), check input5 > 10 : inline warning "Must be over 10", col 0,
                                   hint "check someNumber > 10 : inline warning <message>", mask decimal;
        input6 "Inline info"     : Int, text_field(12), check input6 > 10 : inline info "Must be over 10", col 0,
                                   hint "check someNumber > 10 : inline info <message>", mask decimal;
    };
    footer {
        button(save);
    };
}

