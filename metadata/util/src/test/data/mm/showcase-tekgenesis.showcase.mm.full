package tekgenesis.showcase;

form AddressForm "Address Form"
    : Address
{
    header {
        message(title), col 6;
         "View Source" : label, link "/sg/view/source/tekgenesis.showcase.AddressForm", style "pull-right";
    };
    "Id"          : id, internal, optional;
    "Street"      : street, text_field(60);
    "City"        : city, text_field(40);
    "State"       : state, text_field(40), hide when city == "CABA", optional when city == "CABA", reset;
    "Postal Code" : zip, text_field, optional;
    "Country"     : country, text_field(30);
    footer {
        button(save);
        button(cancel);
        button(delete), style "pull-right";
    };
}

form AddressShowcaseForm "Address" 
    on_load load
{
    header {
        message(title), col 6;
         "View Source" : label, link "/view/source/tekgenesis.showcase.TextFieldShowcaseForm", style "pull-right";
    };
    horizontal {
        vertical, col 8 {
            horizontal, col 0, label_col 4 {
                 "Pais"      : text_field(255), col 6, label_col 4;
                 "Provincia" : text_field(255), col 6, label_col 4;
            };
             "Ciudad"    : input_group, col 0 {
                 "Text Field 1" : text_field(255), col 6;
                 "Text Field 2" : text_field(255), col 6, hint "Codigo postal";
            };
             "Direccion" : input_group, col 0 {
                 "Text Field 1" : text_field(255), col 4, hint "Nombre de la calle", placeholder "Buscar...";
                 "Text Field 2" : text_field(255), col 2, hint "Numero";
                 "Text Field 2" : text_field(255), col 2, hint "Piso";
                 "Text Field 2" : text_field(255), col 2, hint "Depto/Oficina";
            };
             "Otros"     : text_field(255), col 0;
             "Telefono"  : text_field(255), col 0;
             "Entre"     : input_group, col 0 {
                 "Text Field 1" : text_field(255), col 6, hint "Calle 1";
                 "Text Field 2" : text_field(255), col 6, hint "Calle 2";
            };
        };
        vertical, col 4 {
            map    : String, map, col 0, style "no-label";
        };
    };
}

