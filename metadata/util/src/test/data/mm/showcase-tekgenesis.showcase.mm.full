form ChartShowcase "Charts" 
    on_load data
{
    header {
        message(title), col 6;
         "View Source" : label, link "/sg/view/source/tekgenesis.showcase.ChartShowcase", style "pull-right";
    };
          "Append Chart Values" : button, on_click more;
    ref   "Some value"          : Real, text_field(30), default 0;
    div   "Other values"        : Real, text_field(30), default 1;
    stack "Stack?"              : toggle_button, default true, on_change stack, style "marginRight10";
    horizontal {
        line   "Line"                         : String, chart(line), col 6 {
            a                   : text_field(255);
            beavis   "Beavis"   : Real, text_field(30);
            butthead "Butthead" : Real, text_field(30), is beavis + ref;
        };
        column "Column (No hover, no legend)" : String, chart, col 6 {
            b           : text_field(255);
            cain "Cain" : Real, text_field(30), is abel / div;
            abel "Abel" : Real, text_field(30);
        };
    };
    label "Label"               : text_field(255), default "Label";
    bar   "Bar"                 : String, chart(bar) {
        c             : text_field(255), is (label + " ") + tom;
        tom   "Tom"   : Real, text_field(30);
        jerry "Jerry" : Real, text_field(30);
    };
          "Next Rnds"           : button, on_click nextRnds;
    pie   "Pie"                 : String, chart(pie) {
        d                   : text_field(255), is (label + " ") + superman;
        superman "Superman" : Real, text_field(30);
        key                 : String, internal;
    };
    horizontal, style "navbar-fixed-bottom" {
        resolution3 "1280x720" : button, on_click size1280, style "pull-right marginRight10";
        resolution2 "854x480"  : button, on_click size854, style "pull-right";
        resolution1 "640x480"  : button, on_click size640, style "pull-right";
        resolution0 "Auto"     : button, on_click sizeAuto, style "pull-right";
    };
}

