form ClientShowcase "Client" 
    on_load load
{
    header {
        message(title), col 12;
    };
    message(warning), default "Confirmar la direccion del cliente en el mapa";
    horizontal, col 6, hide, offset_col 3 {
        indicators    : String, section, col 9 {
            indicatorIcon    : String, internal, optional;
            indicator        : label, icon_expr indicatorIcon;
        };
        reference     : vertical, col 3, offset_col 2, style "indicatorReferences" {
            goldIcon   " " : label, icon certificate, style "gold", tooltip "High";
            silverIcon " " : label, icon certificate, style "silver", tooltip "Medium";
            bronzeIcon " " : label, icon certificate, style "bronze", tooltip "Low";
        };
    };
    horizontal {
        vertical, col 10 {
             "Identificacion" : input_group, col 0 {
                String, combo_box, col 6;
                text_field, col 6;
            };
             "Nombre"         : input_group, col 0 {
                text_field, col 6, hint "Nombre de pila";
                text_field, col 6, hint "Apellido";
            };
             "Contacto"       : input_group, col 0 {
                text_field, col 6, hint "E-Mail", placeholder "email@ejemplo.com";
                text_field, col 6, hint "Telefono";
            };
             "Otros"          : input_group, col 0 {
                String, combo_box, col 2, hint "Sexo";
                date_box, col 3, hint "Fecha de Nacimiento";
                text_field, col 5, hint "CUIL/CUIT";
            };
        };
